{
  "name": "cobbv2",
  "version": "2.2.9",
  "main": "dist",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "build": "rimraf dist && babel src -d dist",
    "test": "mocha -R spec --require babel-core/register --require babel-polyfill ./src/**/**.test.js ./src/**.test.js ",
    "lint": "eslint src",
    "dev": "babel-node ./test/dev",
    "order": "babel-node cmd.js --config ask_config.env --dev --order",
    "start": "rimraf dist && babel src -d dist && babel-node run.js --config bid_config"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-preset-flow": "^6.23.0",
    "bignumber.js": "^6.0.0",
    "bluebird": "^3.5.1",
    "cobinhood-api-node": "ssh://git@bitbucket.org:uselessmining/cobinhood-api-node.git",
    "colors": "^1.3.0",
    "commander": "^2.14.1",
    "eslint-plugin-flowtype": "^2.49.3",
    "git-rev-sync": "^1.12.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.17",
    "raven": "^2.6.2",
    "redux": "^3.7.2",
    "winston": "^3.0.0",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-zavatta": "^6.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2"
  }
}
